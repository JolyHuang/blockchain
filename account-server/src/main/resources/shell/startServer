#!/bin/sh

JAVA_OPTIONS="${JAVA_OPTIONS} -Dfile.encoding=UTF-8"
JAVA_VM="-server"
MEM_ARGS="-Xms2048m -Xmx2048m"
APP_HOME=/home/blockchain/account-server

nohup java ${JAVA_VM} ${MEM_ARGS} ${BtcDaemonNotifierJAVA_OPTIONS} -jar $APP_HOME/account-server.jar --spring.config.name=application-test 1>>$APP_HOME/account-server.out 2>&1 &